#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5599bb4f46f0 .scope module, "shifting_mantissa_test" "shifting_mantissa_test" 2 1;
 .timescale 0 0;
v0x5599bb514c00_0 .var "mantissa", 23 0;
v0x5599bb514cc0_0 .net "shift", 4 0, v0x5599bb514ac0_0;  1 drivers
S_0x5599bb4f4870 .scope module, "DUT" "shifting_matisaa" 2 4, 3 1 0, S_0x5599bb4f46f0;
 .timescale 0 0;
    .port_info 0 /INPUT 24 "mantissa"
    .port_info 1 /OUTPUT 5 "shift"
v0x5599bb4f4a40_0 .net "mantissa", 23 0, v0x5599bb514c00_0;  1 drivers
v0x5599bb514ac0_0 .var "shift", 4 0;
E_0x5599bb4f0740 .event edge, v0x5599bb514ac0_0, v0x5599bb4f4a40_0;
    .scope S_0x5599bb4f4870;
T_0 ;
    %wait E_0x5599bb4f0740;
    %pushi/vec4 24, 0, 5;
    %store/vec4 v0x5599bb514ac0_0, 0, 5;
T_0.0 ;
    %load/vec4 v0x5599bb4f4a40_0;
    %load/vec4 v0x5599bb514ac0_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %part/u 1;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %pushi/vec4 0, 0, 5;
    %load/vec4 v0x5599bb514ac0_0;
    %cmp/u;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz T_0.1, 8;
    %load/vec4 v0x5599bb514ac0_0;
    %subi 1, 0, 5;
    %store/vec4 v0x5599bb514ac0_0, 0, 5;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x5599bb514ac0_0;
    %cmpi/e 0, 0, 5;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5599bb514ac0_0, 0, 5;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 24, 0, 5;
    %load/vec4 v0x5599bb514ac0_0;
    %sub;
    %addi 1, 0, 5;
    %store/vec4 v0x5599bb514ac0_0, 0, 5;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5599bb4f46f0;
T_1 ;
    %delay 2, 0;
    %pushi/vec4 209715, 0, 24;
    %store/vec4 v0x5599bb514c00_0, 0, 24;
    %delay 2, 0;
    %pushi/vec4 8388608, 0, 24;
    %store/vec4 v0x5599bb514c00_0, 0, 24;
    %delay 2, 0;
    %vpi_call 2 8 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5599bb4f46f0;
T_2 ;
    %vpi_call 2 11 "$dumpfile", "shifting_mantissa_test.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5599bb4f46f0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "shifting_mantissa_test.v";
    "shifting_matissa.v";
